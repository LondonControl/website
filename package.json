    "clsx": "^2.1.0",
    "cookies-next": "^4.1.1",
    "embla-carousel-react": "^8.0.0-rc22",
    "lucide-react": "^0.316.0",
    "moment": "^2.30.1",
    "next": "^14.1.0",
    "next-seo": "^6.4.0",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.2.1",
